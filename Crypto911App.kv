<Menu>:

    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'pexels.jpg'
    Label:
        markup: True

        text:"[color=#f207a8][b]22 Cryptos[/b][/color]"
        font_size: 70
        pos_hint: {"x": .02, "y": .2}
        outline_color: 140, 17, 52
        outline_width: 7

    Label:
        markup: True
        text:"[color=#f207a8][b]by Fomodin.com [/b][/color]"
        font_size: 22
        pos_hint: {"x": .04, "y": .1}
        outline_color: 140, 17, 52
        outline_width: 7


    Button:
        markup: True
        text:"[size=35][b]Start Game[/b][/size]"
        size_hint: .5, .15
        pos_hint: {"x": .25, "y": .3}
        color: (0,0,0)
        background_color: 255, 0, 0
        on_press: root.manager.current = "question1"





